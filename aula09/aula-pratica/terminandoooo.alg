algoritmo "semnome"
// Função :
// Autor :
// Data : 13/09/2017
// Seção de Declarações 
var
   N, cont : inteiro
inicio
      escreva ("Informe um numero: ")
      leia (N)
      cont <- N
      multi <-
      Repita
            cont <- cont - 1
            escreval (cont)
      Ate (cont < 1)
fimalgoritmo
